{
  "name": "memoryboard-api-typescript-one",
  "version": "0.0.1",
  "license": "MIT",
  "prisma": {
    "schema": "src/shared/infra/prisma/schema.prisma"
  },
  "scripts": {
    "envDev": "env-cmd -f .env.dev --",
    "envTest": "env-cmd -f .env.test --",
    "db:start": "docker start memoryboard_database",
    "db:stop": "docker stop memoryboard_database",
    "db:test:start": "docker start memoryboard_test_database",
    "db:test:stop": "docker stop memoryboard_test_database",
    "server:dev": "yarn envDev ts-node-dev --inspect --respawn --transpile-only --ignore-watch node_modules -r tsconfig-paths/register src/shared/infra/server/server.ts",
    "prisma:merge": "prisma-merge --baseFile=src/shared/infra/prisma/base.schema.prisma --schemaFilePatterns=src/modules/**/infra/prisma/schemas/*.schema.prisma --outputFile=src/shared/infra/prisma/schema.prisma",
    "prisma:dev:migrate": "yarn prisma:merge && yarn envDev prisma migrate dev --schema=src/shared/infra/prisma/schema.prisma",
    "prisma:test:migrate": "yarn prisma:merge && yarn envTest prisma migrate dev --schema=src/shared/infra/prisma/schema.prisma",
    "prisma:dev:studio": "yarn envDev prisma studio",
    "prisma:test:studio": "yarn envTest prisma studio",
    "prisma:dev:migrate:reset": "yarn envDev yarn prisma migrate reset",
    "prisma:test:migrate:reset": "yarn envTest yarn prisma migrate reset",
    "test": "yes | yarn prisma:test:migrate:reset && yarn prisma:test:migrate && yarn envTest jest --runInBand --detectOpenHandles --forceExit",
    "seed:dev:createAdmin": "yarn envDev ts-node-dev src/shared/infra/prisma/seeds/createAdmin.ts"
  },
  "dependencies": {
    "@prisma/client": "^2.21.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dayjs": "^1.10.4",
    "dotenv": "^8.2.0",
    "env-cmd": "^10.1.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.2",
    "nanoid": "^3.1.22",
    "pg": "^8.6.0",
    "reflect-metadata": "^0.1.13",
    "socket.io": "^4.1.1",
    "tsyringe": "^4.5.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.23",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/multer": "^1.4.5",
    "@types/node": "^15.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "eslint": "^7.25.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-import-helpers": "^1.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "prisma": "^2.21.2",
    "prisma-merge": "^0.2.0",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.5",
    "ts-node-dev": "^1.1.6",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.4"
  }
}
